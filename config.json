{
  "commands": {
    ":!test": {
      "response": "this is a test",
      "cooldown": 5.0,
      "sub_only": false
    },
    ":!anothercmd": {
      "response": "Another response",
      "cooldown": 5,
      "sub_only": false
    },
    ":!uptime": {
      "response": "${UPTIME}$",
      "cooldown": 90.0,
      "sub_only": false
    },
    ":!mycommand": {
      "response": "Custom response",
      "cooldown": 5,
      "sub_only": false
    },
    ":!Hello": {
      "response": "Hello ${USER}$",
      "cooldown": 5,
      "sub_only": false
    }
  },
  "general": {
    "commandrate": {
      "cmd_limit": 100,
      "time_limit": 30
    },
    "special_access": {
      "admin": true,
      "staff": true,
      "global_mod": true,
      "mod": true
    },
    "oauth": "oauth:key_from_http://twitchapps.com/tmi",
    "botname": "bot_account_name",
    "port": 6667,
    "channel": "#channel_name",
    "server": "irc.twitch.tv"
  }
}
